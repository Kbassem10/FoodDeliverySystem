            <Font name="Goudy Old Style Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="primaryButton" onAction="#switchToAdminMainMenu" prefHeight="36.0" prefWidth="69.0" text="Admin" />
      <Button fx:id="primaryButton1" onAction="#switchToCustomerMenu" prefHeight="35.0" prefWidth="67.0" text="Customer" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
